{
  "port": 3000,
  "amountOfBuildThreads": "cpuHalf",
  "meta": {
    "language": "en",
    "siteName": "Future Frontend",
    "url": "https://future-frontend.com"
  },
  "scripts": [
    {
      "type": "text/javascript",
      "src": "https://unpkg.com/sidewind@7.6.0/dist/sidewind.umd.production.min.js"
