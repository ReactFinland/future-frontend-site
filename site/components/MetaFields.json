[
  {
    "type": "link",
    "attributes": {
      "rel": "icon",
      "href": "/images/icon.ico",
      "sizes": "any"
    }
  },
  {
    "type": "link",
    "attributes": {
      "rel": "icon",
      "href": "/images/icon.svg",
      "type": "image/svg+xml"
    }
  },
  {
    "type": "link",
    "attributes": {
      "rel": "canonical",
      "href": {
        "utility": "concat",
        "parameters": [
          { "utility": "get", "parameters": ["context", "meta.url"] },
          { "utility": "get", "parameters": ["context", "meta.pagePath"] }
        ]
      }
    }
  },
  {
    "type": "meta",
    "attributes": {
      "name": "pagepath",
      "content": {
        "utility": "get",
        "parameters": ["context", "meta.pagePath"]
      }
    }
  },
  {
    "_todo": "load this only where the venue map is used",
    "_reference": "https://web.dev/defer-non-critical-css/",
    "type": "link",
    "attributes": {
      "rel": "preload",
      "href": "https://unpkg.com/leaflet@1.9.3/dist/leaflet.css",
      "integrity": "sha384-o/2yZuJZWGJ4s/adjxVW71R+EO/LyCwdQfP5UWSgX/w87iiTXuvDZaejd3TsN7mf",
      "crossorigin": "anonymous",
      "as": "style",
      "onload": "this.onload=null;this.rel='stylesheet'"
    }
  },
  {
    "type": "meta",
    "attributes": {
      "charset": "UTF-8",
      "name": "viewport",
