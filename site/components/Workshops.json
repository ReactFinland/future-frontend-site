        "bindToProps": {
          "children": {
            "utility": "concat",
            "parameters": [
              {
                "utility": "get",
                "parameters": ["props", "title"]
              }
            ]
          }
        }
      },
      {
        "type": "div",
        "class": "flex flex-row gap-4",
        "children": [
          {
            "type": "div",
            "class": "flex flex-col gap-4",
            "children": [
              {
                "type": "div",
                "class": "text-md whitespace-nowrap",
                "children": {
                  "utility": "concat",
                  "parameters": [
                    {
                      "utility": "getDate",
                      "parameters": [
                        {
                          "utility": "get",
                          "parameters": [
                            "props",
                            "day"
                          ]
                        }
                      ]
                    },
                    " (",
                    {
                      "utility": "offsetByTimezone",
                      "parameters": [
                        {
                          "utility": "get",
                          "parameters": [
                            "props",
                            "begin"
                          ]
                        }
                      ]
                    },
                    "â€“",
                    {
                      "utility": "offsetByTimezone",
                      "parameters": [
                        {
                          "utility": "get",
                          "parameters": [
                            "props",
                            "end"
                          ]
                        }
                      ]
                    },
                    ")"
                  ]
                }
              },
              {
                "type": "Heading",
                "props": {
                  "level": 3,
                  "class": "sr-only"
                },
                "bindToProps": {
                  "children": {
                    "utility": "concat",
                    "parameters": ["Instructor", {
                      "utility": "pluralize",
                      "parameters": [{
                        "utility": "get",
                        "parameters": ["props", "speakers"]
                      }]
                    }]
                  }
                }
              },
              {
                "type": "MiniSpeakers",
                "bindToProps": {
                  "children": {
                    "utility": "get",
                    "parameters": [
                      "props",
                      "speakers"
                    ]
                  },
                  "captionClass": "text-center",
                  "imgClass": "mx-auto rounded-full w-16"
                }
              },
              {
                "type": "Heading",
                "props": {
                  "class": "sr-only",
                  "children": "Location",
                  "level": 3
                }
              },
              {
                "type": "Link",
                "bindToProps": {
                  "children": {
                    "utility": "concat",
                    "parameters": [
                      "Location: ",
                      {
                        "utility": "get",
                        "parameters": ["props", "location.name"]
                      },
                      ", ",
                      {
                        "utility": "get",
                        "parameters": ["props", "location.address"]
                      },
                      ", ",
                      {
                        "utility": "get",
                        "parameters": ["props", "location.city"]
                      }
                    ]
                  },
                  "href": {
                    "utility": "get",
                    "parameters": ["props", "location.social.googleMaps"]
                  }
                }
              }
            ]
          },
          {
            "type": "div",
            "children": [
              {
                "type": "Heading",
                "props": {
                  "level": 3,
                  "class": "text-3xl sr-only",
