                }
              },
              {
                "type": "Markdown",
                "bindToProps": {
                  "inputText": {
                    "utility": "get",
                    "parameters": ["props", "description"]
                  }
                },
                "props": {
                  "class": "prose lg:prose-xl",
                  "type": "div"
                }
              }
            ]
          }
        ]
      }
    ]
  }]
