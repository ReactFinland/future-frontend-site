{
  "type": "div",
  "class": "flex flex-col gap-8",
  "children": [
    {
      "type": "Heading",
      "props": {
        "level": 2,
        "children": "All sponsors",
        "class": "sr-only"
      }
    },
    {
      "type": "div",
      "class": "flex flex-col gap-4",
      "children": [
        {
          "type": "Heading",
          "props": {
            "level": 3,
            "children": "Gold sponsors",
            "class": "text-6xl"
          }
        },
        {
          "type": "div",
          "class": "grid grid-cols-2 gap-8",
          "children": [
            {
              "type": "MiniSponsors",
              "bindToProps": {
                "children": {
                  "utility": "get",
                  "parameters": [
                    "props",
                    "conference.goldSponsors"
                  ]
                },
                "aClass": "my-auto"
              }
            }
          ]
        },
        {
          "type": "Link",
          "visibleIf": [
            {
              "utility": "lessThan",
              "parameters": [
                {
                  "utility": "get",
                  "parameters": ["props", "conference.goldSponsors"]
                },
                2
              ]
            }
          ],
          "props": {
            "children": "Become a gold sponsor",
            "href": "for-sponsors"
          }
        }
      ]
    },
    {
      "type": "div",
      "class": "flex flex-col gap-4",
      "children": [
        {
          "type": "Heading",
          "props": {
            "level": 3,
            "children": "Silver sponsors",
            "class": "text-4xl"
          }
        },
        {
          "type": "div",
          "class": "grid grid-cols-4 gap-8",
          "children": [
            {
              "type": "MiniSponsors",
              "bindToProps": {
                "children": {
                  "utility": "get",
