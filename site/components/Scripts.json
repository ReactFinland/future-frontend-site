{
  "foreach": [{ "utility": "get", "parameters": ["context", "scripts"] }, {
    "visibleIf": [
      { "utility": "get", "parameters": ["props", "loadInitially", true] }
    ],
    "type": "script",
    "attributes": {
      "type": {
        "utility": "get",
        "parameters": ["props", "type"]
      },
      "src": {
        "utility": "get",
        "parameters": ["props", "src"]
      }
    }
  }]
}
