{
  "foreach": [{ "utility": "get", "parameters": ["props", "children"] }, {
    "type": "div",
    "class": "flex flex-col gap-2",
    "children": [
      {
        "type": "h2",
        "class": "text-2xl flex flex-row gap-2 font-primary text-primary",
        "children": [
          {
            "type": "span",
            "children": {
              "utility": "get",
              "parameters": ["props", "day"]
            }
          },
          {
            "type": "span",
            "children": "-"
          },
          {
            "type": "span",
            "children": {
              "utility": "get",
              "parameters": ["props", "description"]
            }
          }
        ]
      },
      {
        "type": "div",
        "class": "flex flex-col gap-4",
        "children": [
          {
            "foreach": [
              { "utility": "get", "parameters": ["props", "intervals"] },
              {
                "type": "div",
                "class": "flex flex-row gap-8",
                "children": [
                  {
                    "type": "div",
                    "class": "font-monospace text-lg",
                    "children": [
                      {
                        "type": "span",
                        "children": {
                          "utility": "offsetByTimezone",
                          "parameters": [
                            {
                              "utility": "get",
                              "parameters": ["props", "begin"]
                            }
                          ]
                        }
                      },
                      {
                        "type": "span",
                        "children": "-"
                      },
                      {
                        "type": "span",
                        "children": {
                          "utility": "offsetByTimezone",
                          "parameters": [
                            {
                              "utility": "get",
                              "parameters": ["props", "end"]
                            }
                          ]
                        }
                      }
                    ]
                  },
                  {
                    "type": "div",
                    "class": "col-span-3",
                    "children": [
                      {
                        "visibleIf": [{
                          "utility": "get",
                          "parameters": ["props", "title"]
                        }],
                        "type": "h3",
                        "class": "text-lg",
                        "children": {
                          "utility": "get",
                          "parameters": ["props", "title"]
                        }
                      },
                      {
                        "foreach": [
                          {
                            "utility": "get",
                            "parameters": ["props", "sessions"]
                          },
                          {
                            "type": "div",
                            "class": "flex flex-row gap-2",
                            "children": [
                              {
                                "type": "div",
                                "children": {
                                  "utility": "get",
                                  "parameters": ["props", "title"]
                                }
                              },
                              {
                                "visibleIf": [
                                  {
                                    "utility": "get",
                                    "parameters": ["props", "speakers"]
                                  }
                                ],
                                "children": " â€“ "
                              },
                              {
                                "type": "MiniSpeakers",
                                "bindToProps": {
                                  "children": {
                                    "utility": "get",
                                    "parameters": [
                                      "props",
                                      "speakers"
                                    ]
                                  },
                                  "imgContainerClass": "flex-row gap-1",
                                  "captionClass": "text-center",
                                  "imgClass": "mx-auto rounded-full w-6"
                                }
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }]
}
