{
  "type": "div",
  "attributes": {
    "x-state": {
      "utility": "concat",
      "parameters": [
        "'",
        {
          "utility": "get",
          "parameters": ["props", "selectedTab"]
        },
        "'"
      ]
    },
    "hidden": "",
    "x-cloak": ""
  },
  "children": [
    {
      "type": "nav",
      "class": "flex flex-row justify-between cursor-pointer text-white",
      "foreach": [{ "utility": "get", "parameters": ["props", "tabs"] }, {
        "type": "div",
        "attributes": {
          "x-attr": "",
          "class": "w-full p-2",
          "@class": {
            "utility": "concat",
            "parameters": [
              "state === '",
              { "utility": "get", "parameters": ["props", "id"] },
              "' ? '",
              { "utility": "tw", "parameters": ["bg-tertiary"] },
              "' : '",
              { "utility": "tw", "parameters": ["bg-primary"] },
              "'"
            ]
          },
          "onclick": {
            "utility": "concat",
            "parameters": [
              "setState('",
              { "utility": "get", "parameters": ["props", "id"] },
              "')"
            ]
          }
        },
        "children": {
          "utility": "render",
          "parameters": [
            {
              "utility": "get",
              "parameters": ["props", "label"]
            }
          ]
        }
      }]
    },
    {
      "type": "div",
      "class": "m-2",
      "foreach": [{ "utility": "get", "parameters": ["props", "tabs"] }, {
        "type": "div",
        "attributes": {
          "x-if": {
            "utility": "concat",
            "parameters": [
              "state === '",
              { "utility": "get", "parameters": ["props", "id"] },
              "'"
            ]
          }
        },
        "children": {
          "utility": "render",
          "parameters": [
            {
              "utility": "get",
              "parameters": ["props", "content"]
            }
          ]
        }
      }]
    }
  ]
}
