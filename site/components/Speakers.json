{
  "type": "div",
  "class": {
    "utility": "get",
    "parameters": ["props", "class", ""]
  },
  "children": [
    {
      "foreach": [{ "utility": "get", "parameters": ["props", "children"] }, {
        "type": "div",
        "children": [
          {
            "type": "figure",
            "class": "float-left mr-4",
            "children": [
              [{
                "type": "img",
                "class": "rounded-full object-contain h-32",
                "attributes": {
                  "alt": { "utility": "get", "parameters": ["props", "name"] },
                  "loading": "lazy",
                  "src": {
                    "utility": "rewriteImageSource",
                    "parameters": [
                      {
                        "utility": "get",
                        "parameters": ["props", "image.url"]
                      }
                    ]
                  },
                  "title": {
                    "utility": "get",
                    "parameters": ["props", "tagline"]
                  },
                  "width": "128",
                  "height": "128"
                }
              }, {
                "type": "figcaption",
                "class": "sr-only",
                "children": {
                  "utility": "get",
                  "parameters": ["props", "name"]
                }
              }]
            ]
          },
          {
            "type": "div",
            "class": "flex flex-col gap-4",
            "children": [
              {
                "type": "div",
                "children": [
                  {
                    "type": "Heading",
                    "props": {
                      "level": 2,
                      "class": "text-4xl"
                    },
                    "bindToProps": {
                      "children": {
                        "utility": "get",
                        "parameters": ["props", "name"]
                      }
                    }
                  },
                  {
                    "type": "p",
                    "class": "text-2xl text-gray-500 font-light",
                    "children": {
                      "utility": "get",
                      "parameters": ["props", "tagline"]
                    }
                  }
                ]
              },
              {
                "type": "div",
                "class": "flex flex-row flex-wrap gap-4",
                "children": [
                  {
                    "type": "Link",
                    "bindToProps": {
                      "children": "Homepage",
                      "href": {
                        "utility": "get",
                        "parameters": ["props", "social.homepage"]
                      }
                    }
                  },
                  {
                    "type": "Link",
                    "bindToProps": {
                      "children": "GitHub",
                      "href": {
                        "utility": "get",
                        "parameters": ["props", "social.github"]
                      }
                    }
                  },
                  {
                    "type": "Link",
                    "bindToProps": {
                      "children": "Twitter",
                      "href": {
                        "utility": "get",
                        "parameters": ["props", "social.twitter"]
                      }
                    }
                  },
                  {
                    "type": "Link",
                    "bindToProps": {
                      "children": "LinkedIn",
                      "href": {
                        "utility": "get",
                        "parameters": ["props", "social.linkedin"]
                      }
                    }
                  }
                ]
              },
              {
                "type": "Markdown",
                "visibleIf": [{
                  "utility": "get",
                  "parameters": ["props", "about"]
                }],
                "bindToProps": {
                  "inputText": {
                    "utility": "get",
                    "parameters": ["props", "about"]
                  }
                },
                "props": {
                  "class": "prose lg:prose-xl",
                  "type": "div"
                }
              },
              {
                "type": "div",
                "visibleIf": [{
                  "utility": "get",
                  "parameters": ["props", "talks"]
                }],
                "children": [
                  {
                    "type": "Heading",
                    "props": {
                      "level": 3,
                      "class": "text-2xl"
                    },
                    "bindToProps": {
                      "children": {
                        "utility": "concat",
                        "parameters": ["Talk", {
                          "utility": "pluralize",
                          "parameters": [{
                            "utility": "get",
                            "parameters": ["props", "talks"]
                          }]
                        }]
                      }
                    }
                  },
                  {
                    "type": "div",
                    "class": "flex flex-col gap-4",
                    "children": [
                      {
                        "foreach": [
                          {
                            "utility": "get",
                            "parameters": ["props", "talks"]
                          },
                          {
                            "type": "div",
                            "class": "flex flex-col gap-2",
                            "children": [
                              {
                                "type": "h4",
                                "class": "text-xl",
                                "children": {
                                  "utility": "get",
                                  "parameters": ["props", "title"]
                                }
                              },
                              {
                                "type": "Markdown",
                                "bindToProps": {
                                  "inputText": {
                                    "utility": "get",
                                    "parameters": ["props", "description"]
                                  }
                                },
                                "props": {
                                  "class": "prose lg:prose-xl",
                                  "type": "div"
                                }
                              },
                              {
                                "type": "div",
                                "class": "flex flex-row gap-4",
                                "children": [
                                  {
                                    "type": "Link",
                                    "bindToProps": {
                                      "children": "Slides",
                                      "href": {
                                        "utility": "get",
                                        "parameters": ["props", "urls.slides"]
                                      }
                                    }
                                  },
                                  {
                                    "type": "Link",
                                    "bindToProps": {
                                      "children": "Video",
                                      "href": {
                                        "utility": "get",
                                        "parameters": ["props", "urls.video"]
                                      }
                                    }
                                  },
                                  {
                                    "type": "Link",
                                    "bindToProps": {
                                      "children": "Drawing",
                                      "href": {
                                        "utility": "get",
                                        "parameters": ["props", "urls.drawing"]
                                      }
                                    }
                                  },
                                  {
                                    "type": "Link",
                                    "bindToProps": {
                                      "children": "Demo",
                                      "href": {
                                        "utility": "get",
                                        "parameters": ["props", "urls.demo"]
                                      }
                                    }
                                  }
                                ]
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              },
              {
                "type": "div",
                "visibleIf": [{
                  "utility": "get",
                  "parameters": ["props", "workshops_disabled"]
                }],
                "children": [
                  {
                    "type": "Heading",
                    "props": {
                      "level": 3,
                      "children": "Workshops",
                      "class": "text-2xl"
                    }
                  },
                  {
                    "type": "div",
                    "class": "flex flex-col gap-4",
                    "children": [
                      {
                        "foreach": [
                          {
                            "utility": "get",
                            "parameters": ["props", "workshops"]
                          },
                          {
                            "type": "div",
                            "children": [
                              {
                                "type": "h4",
                                "class": "text-xl",
                                "children": {
                                  "utility": "get",
                                  "parameters": ["props", "title"]
                                }
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }]
    }
  ]
}
