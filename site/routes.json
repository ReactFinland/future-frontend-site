{
  "/": {
    "layout": "siteIndex",
    "context": {
      "meta": {
        "hideTitle": true,
        "title": "Future Frontend",
        "description": "Future Frontend is a conference series bringing together developers in Finland to reimagine the future of frontend development."
      }
    },
    "dataSources": {
      "conference": {
        "operation": "queryData",
        "parameters": ["future-frontend-2026", "index"]
      }
    },
    "scripts": [{ "name": "venue", "loadedLater": true }]
  },
  "blog": {
    "layout": "blogIndex",
    "context": {
      "meta": {
        "title": "Blog",
        "description": "Future Frontend blog"
      }
    },
    "dataSources": {
      "blogPosts": {
        "operation": "indexMarkdown",
        "parameters": ["./blogPosts"]
      }
    },
    "expand": {
      "matchBy": {
        "name": "blogPosts",
        "indexer": {
          "operation": "indexMarkdown",
          "parameters": ["./blogPosts"]
        },
        "slug": "data.slug"
      },
      "layout": "blogPage",
      "dataSources": {
        "document": {
          "operation": "processMarkdown",
          "parameters": [
            { "parseHeadmatter": true }
          ]
        }
      },
      "context": {
        "meta": {
          "title": {
            "utility": "get",
            "parameters": ["context", "document.data.title"]
          },
          "description": {
            "utility": "get",
            "parameters": ["context", "document.data.description"]
          }
        }
      }
    }
  },
  "buy-tickets": {
    "layout": "buyTickets",
    "context": {
      "meta": {
        "title": "Buy tickets",
        "description": "Buy tickets for Future Frontend 2026"
      }
    }
  },
