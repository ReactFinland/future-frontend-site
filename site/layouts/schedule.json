{
  "type": "BaseLayout",
  "bindToProps": {
    "selectedTab": "animals"
  },
  "props": {
    "content": [
      {
        "type": "div",
        "class": "flex flex-col gap-8",
        "children": [
          {
            "type": "Tabs",
            "bindToProps": {
              "selectedTab": {
                "utility": "get",
                "parameters": ["props", "selectedTab"]
              }
            },
            "props": {
              "tabs": [{
                "id": "animals",
                "label": "Animals",
                "content": "Animals tab"
              }, {
                "id": "languages",
                "label": "Languages",
                "content": "Languages tab"
              }, {
                "id": "colors",
                "label": "Colors",
                "content": "Colors tab"
              }]
            }
          },
          {
            "type": "Schedule",
            "bindToProps": {
              "children": {
                "utility": "get",
                "parameters": [
                  "context",
                  "schedule"
                ]
              }
            }
          }
        ]
      }
    ]
  }
}
