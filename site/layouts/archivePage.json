{
  "type": "BaseLayout",
  "props": {
    "content": [
      {
        "type": "div",
        "children": [
          {
            "type": "Markdown",
            "bindToProps": {
              "children": {
                "utility": "get",
                "parameters": ["context", "content"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          }
        ]
      },
      {
        "type": "AllSpeakers",
        "bindToProps": {
          "conference": {
            "utility": "get",
            "parameters": [
              "context",
              "conference"
            ]
          }
        }
      },
      {
        "type": "AllSponsors",
        "bindToProps": {
          "conference": {
            "utility": "get",
            "parameters": [
              "context",
              "conference"
            ]
          }
        }
      }
    ]
  }
}
