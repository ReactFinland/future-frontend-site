{
  "type": "BaseLayout",
  "props": {
    "header": [
      {
        "type": "h2",
        "class": "text-xl md:text-4xl font-extralight",
        "children": {
          "utility": "get",
          "parameters": ["context", "conference.slogan"]
        }
      },
      {
        "type": "div",
        "class": "flex flex-row justify-between",
        "children": [
          {
            "type": "h3",
            "class": "text-xl md:text-4xl font-extralight",
            "children": [
              {
                "children": {
                  "utility": "getDate",
                  "parameters": [
                    {
                      "utility": "get",
                      "parameters": [
                        "context",
                        "conference.startDate"
                      ]
                    }
                  ]
                }
              },
              { "children": "-" },
              {
                "children": {
                  "utility": "getDate",
                  "parameters": [
                    {
                      "utility": "get",
                      "parameters": [
                        "context",
                        "conference.endDate"
                      ]
                    }
                  ]
                }
              },
              { "children": "." },
              {
                "children": {
                  "utility": "getYear",
                  "parameters": [
                    {
                      "utility": "get",
                      "parameters": [
                        "context",
                        "conference.endDate"
                      ]
                    }
                  ]
                }
              }
            ]
          },
          {
            "_todo": "resolve this from graphql api",
            "type": "h3",
            "class": "text-xl md:text-4xl font-extralight",
            "children": "PÃ¶rssitalo, Helsinki, Finland"
          }
        ]
      }
    ],
    "content": [
      {
        "type": "div",
        "class": "flex flex-col gap-8",
        "children": [
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "intro"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "venue"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "iframe",
            "class": "mj-w-res-iframe",
            "attributes": {
              "frameborder": "0",
              "loading": "lazy",
              "scrolling": "no",
              "marginheight": "0",
              "marginwidth": "0",
              "src": "https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1984.7442588354022!2d24.9492156!3d60.168412499999995!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x46920bcef020bc67%3A0x8ef326ac78c9c7fb!2sHelsinki%20Stock%20Exchange%20Building!5e0!3m2!1sen!2sfi!4v1671358571207!5m2!1sen!2sfi",
              "width": "100%",
              "height": "400"
            }
          },
          {
            "type": "AllSpeakers",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          },
          {
            "type": "div",
            "children": [
              {
                "type": "h2",
                "class": "text-4xl font-primary text-primary",
                "children": "Buy tickets"
              },
              {
                "type": "LazyLoad",
                "props": {
                  "src": "https://js.tito.io/v2",
                  "children": [
                    {
                      "type": "script",
                      "attributes": {
                        "x-attr": "",
                        "async": "",
                        "@src": "state.src"
                      }
                    },
                    {
                      "type": "tito-widget",
                      "attributes": {
                        "event": "future-frontend/2023"
                      }
                    }
                  ]
                }
              }
            ]
          },
          {
            "type": "div",
            "children": [
              {
                "type": "h2",
                "class": "text-4xl font-primary text-primary",
                "children": "Newsletter"
              },
              {
                "type": "Subscribe"
              }
            ]
          },
          {
            "type": "AllSponsors",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          }
        ]
      }
    ]
  }
}
