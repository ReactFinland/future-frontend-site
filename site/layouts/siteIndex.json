{
  "type": "BaseLayout",
  "props": {
    "content": [
      {
        "type": "header",
        "class": "bg-black py-8",
        "children": [
          {
            "type": "div",
            "class": "sm:mx-auto px-4 py-4 sm:py-8 max-w-3xl flex",
            "children": [
              {
                "type": "div",
                "class": "flex flex-col gap-8 text-white",
                "children": [
                  {
                    "type": "h1",
                    "class": "text-4xl md:text-6xl",
                    "children": [
                      {
                        "type": "span",
                        "children": {
                          "utility": "get",
                          "parameters": ["context", "index.conference.name"]
                        },
                        "class": "align-middle font-primary"
                      }
                    ]
                  },
                  {
                    "type": "h2",
                    "class": "text-xl md:text-4xl font-extralight",
                    "children": {
                      "utility": "get",
                      "parameters": ["context", "index.conference.slogan"]
                    }
                  },
                  {
                    "type": "div",
                    "class": "flex flex-row justify-between",
                    "children": [
                      {
                        "type": "h3",
                        "class": "text-xl md:text-4xl font-extralight",
                        "children": [
                          {
                            "children": {
                              "utility": "getDate",
                              "parameters": [
                                {
                                  "utility": "get",
                                  "parameters": [
                                    "context",
                                    "index.conference.startDate"
                                  ]
                                }
                              ]
                            }
                          },
                          { "children": "-" },
                          {
                            "children": {
                              "utility": "getDate",
                              "parameters": [
                                {
                                  "utility": "get",
                                  "parameters": [
                                    "context",
                                    "index.conference.endDate"
                                  ]
                                }
                              ]
                            }
                          },
                          { "children": "." },
                          {
                            "children": {
                              "utility": "getYear",
                              "parameters": [
                                {
                                  "utility": "get",
                                  "parameters": [
                                    "context",
                                    "index.conference.endDate"
                                  ]
                                }
                              ]
                            }
                          }
                        ]
                      },
                      {
                        "_todo": "resolve this from graphql api",
                        "type": "h3",
                        "class": "text-xl md:text-4xl font-extralight",
                        "children": "Helsinki, Finland"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "type": "div",
        "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl flex flex-col gap-8",
        "children": [
          {
            "type": "div",
            "children": [
              {
                "type": "h2",
                "class": "text-6xl",
                "children": "Speakers"
              },
              {
                "type": "Speakers"
              }
            ]
          },
          {
            "type": "AllSponsors",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "index.conference"
                ]
              }
            }
          }
        ]
      }
    ]
  }
}
