{
  "type": "BaseLayout",
  "props": {
    "header": [
      {
        "type": "Heading",
        "props": {
          "hideAnchor": true,
          "level": 2,
          "class": "text-xl md:text-4xl font-extralight"
        },
        "bindToProps": {
          "children": {
            "utility": "get",
            "parameters": ["context", "conference.slogan"]
          }
        }
      },
      {
        "type": "div",
        "class": "flex flex-row justify-between",
        "children": [
          {
            "type": "Heading",
            "props": {
              "hideAnchor": true,
              "level": 3,
              "class": "text-xl md:text-4xl font-extralight"
            },
            "bindToProps": {
              "children": {
                "utility": "concat",
                "parameters": [
                  {
                    "utility": "getDate",
                    "parameters": [
                      {
                        "utility": "get",
                        "parameters": [
                          "context",
                          "conference.startDate"
                        ]
                      }
                    ]
                  },
                  "-",
                  {
                    "utility": "getDate",
                    "parameters": [
                      {
                        "utility": "get",
                        "parameters": [
                          "context",
                          "conference.endDate"
                        ]
                      }
                    ]
                  },
                  ".",
                  {
                    "utility": "getYear",
                    "parameters": [
                      {
                        "utility": "get",
                        "parameters": [
                          "context",
                          "conference.endDate"
                        ]
                      }
                    ]
                  }
                ]
              }
            }
          },
          {
            "_todo": "resolve this from graphql api",
            "type": "Heading",
            "props": {
              "hideAnchor": true,
              "level": 3,
              "class": "text-xl md:text-4xl font-extralight",
              "children": "PÃ¶rssitalo, Helsinki, Finland"
            }
          }
        ]
      }
    ],
    "content": [
      {
        "type": "div",
        "class": "flex flex-col gap-8",
        "children": [
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "intro"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "venue"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "div",
            "class": "w-full h-96 bg-map bg-center",
            "attributes": {
              "id": "venue-map"
            }
          },
          {
            "type": "LazyLoad",
            "props": {
              "src": "/venue.js",
              "children": [
                {
                  "type": "script",
                  "attributes": {
                    "x-attr": "",
                    "async": "",
                    "@src": "state.src",
                    "type": "module"
                  }
                }
              ]
            }
          },
          {
            "type": "AllSpeakers",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          },
          {
            "type": "div",
            "children": [
              {
                "type": "Link",
                "props": {
                  "children": [
                    {
                      "type": "Heading",
                      "props": {
                        "level": 2,
                        "children": "Buy tickets",
                        "class": "text-4xl font-primary text-primary"
                      }
                    }
                  ],
                  "href": "https://ti.to/future-frontend/2023"
                }
              },
              {
                "type": "LazyLoad",
                "props": {
                  "src": "https://js.tito.io/v2",
                  "children": [
                    {
                      "type": "script",
                      "attributes": {
                        "x-attr": "",
                        "async": "",
                        "@src": "state.src"
                      }
                    },
                    {
                      "type": "tito-widget",
                      "attributes": {
                        "event": "future-frontend/2023"
                      }
                    }
                  ]
                }
              }
            ]
          },
          {
            "type": "div",
            "class": "flex flex-col gap-4",
            "children": [
              {
                "type": "Heading",
                "props": {
                  "level": 2,
                  "children": "Subscribe to the mailing list",
                  "class": "text-4xl font-primary"
                }
              },
              {
                "type": "Subscribe"
              }
            ]
          },

          {
            "type": "AllSponsors",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          }
        ]
      }
    ]
  }
}
