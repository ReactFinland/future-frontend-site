{
  "type": "BaseLayout",
  "props": {
    "header": [
      {
        "type": "Heading",
        "props": {
          "hideAnchor": true,
          "level": 2,
          "class": "text-xl md:text-4xl font-light"
        },
        "bindToProps": {
          "children": {
            "utility": "get",
            "parameters": ["context", "conference.slogan"]
          }
        }
      },
      {
        "type": "div",
        "class": "flex flex-col gap-4",
        "children": [
          {
            "type": "div",
            "class": "flex flex-row justify-between",
            "children": [
              {
                "type": "Heading",
                "props": {
                  "hideAnchor": true,
                  "level": 3,
                  "class": "text-xl md:text-4xl font-extralight"
                },
                "bindToProps": {
                  "children": {
                    "utility": "concat",
                    "parameters": [
                      {
                        "utility": "getDate",
                        "parameters": [
                          {
                            "utility": "get",
                            "parameters": [
                              "context",
                              "conference.startDate"
                            ]
                          }
                        ]
                      },
                      "-",
                      {
                        "utility": "getDate",
                        "parameters": [
                          {
                            "utility": "get",
                            "parameters": [
                              "context",
                              "conference.endDate"
                            ]
                          }
                        ]
                      },
                      ".",
                      {
                        "utility": "getYear",
                        "parameters": [
                          {
                            "utility": "get",
                            "parameters": [
                              "context",
                              "conference.endDate"
                            ]
                          }
                        ]
                      }
                    ]
                  }
                }
              },
              {
                "_todo": "resolve this from graphql api",
                "type": "Heading",
                "props": {
                  "hideAnchor": true,
                  "level": 3,
                  "class": "text-xl md:text-4xl font-extralight",
                  "children": "Conference at PÃ¶rssitalo"
                }
              }
            ]
          },
          {
            "type": "div",
            "class": "flex flex-row justify-between",
            "children": [
              {
                "type": "Heading",
                "props": {
                  "hideAnchor": true,
                  "level": 3,
                  "class": "text-xl md:text-4xl font-extralight"
                },
                "bindToProps": {
                  "children": "6.6-7.6.2023"
                }
              },
              {
                "type": "Heading",
                "props": {
                  "hideAnchor": true,
                  "level": 3,
                  "class": "text-xl md:text-4xl font-extralight",
                  "children": "Workshops"
                }
              }
            ]
          }
        ]
      }
    ],
    "content": [
      {
        "type": "div",
        "class": "flex flex-col gap-8",
        "children": [
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "intro"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "venue"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "div",
            "class": "w-full h-96 bg-map bg-center",
            "attributes": {
              "id": "venue-map"
            }
          },
          {
            "type": "script",
            "attributes": {
              "async": "",
              "src": "./venue.js",
              "type": "module"
            }
          },
          {
            "type": "div",
            "children": [
              {
                "type": "Heading",
                "props": {
                  "level": 2,
                  "children": "Subscribe to our mailing list for the latest news",
                  "class": "text-2xl"
                }
              },
              {
                "type": "Subscribe"
              }
            ]
          },
          {
            "type": "AllSpeakers",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          },
          {
            "type": "AllSponsors",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          }
        ]
      }
    ]
  }
}
