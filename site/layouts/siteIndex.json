{
  "type": "BaseLayout",
  "props": {
    "content": [
      {
        "type": "div",
        "children": [
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "intro"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "div",
            "children": [
              {
                "type": "h2",
                "class": "text-6xl",
                "children": "Speakers"
              },
              {
                "type": "Speakers"
              }
            ]
          },
          {
            "type": "AllSponsors",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "index.conference"
                ]
              }
            }
          }
        ]
      }
    ]
  }
}
