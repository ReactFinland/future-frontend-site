{
  "type": "BaseLayout",
  "props": {
    "header": [
      {
        "type": "h2",
        "class": "text-xl md:text-4xl font-extralight",
        "children": {
          "utility": "get",
          "parameters": ["context", "conference.slogan"]
        }
      },
      {
        "type": "div",
        "class": "flex flex-row justify-between",
        "children": [
          {
            "type": "h3",
            "class": "text-xl md:text-4xl font-extralight",
            "children": [
              {
                "children": {
                  "utility": "getDate",
                  "parameters": [
                    {
                      "utility": "get",
                      "parameters": [
                        "context",
                        "conference.startDate"
                      ]
                    }
                  ]
                }
              },
              { "children": "-" },
              {
                "children": {
                  "utility": "getDate",
                  "parameters": [
                    {
                      "utility": "get",
                      "parameters": [
                        "context",
                        "conference.endDate"
                      ]
                    }
                  ]
                }
              },
              { "children": "." },
              {
                "children": {
                  "utility": "getYear",
                  "parameters": [
                    {
                      "utility": "get",
                      "parameters": [
                        "context",
                        "conference.endDate"
                      ]
                    }
                  ]
                }
              }
            ]
          },
          {
            "_todo": "resolve this from graphql api",
            "type": "h3",
            "class": "text-xl md:text-4xl font-extralight",
            "children": "PÃ¶rssitalo, Helsinki, Finland"
          }
        ]
      }
    ],
    "content": [
      {
        "type": "div",
        "class": "flex flex-col gap-8",
        "children": [
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "intro"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "Markdown",
            "bindToProps": {
              "inputText": {
                "utility": "get",
                "parameters": ["context", "venue"]
              }
            },
            "props": {
              "class": "prose lg:prose-xl",
              "type": "div"
            }
          },
          {
            "type": "div",
            "class": "w-full h-96 bg-slate-200",
            "attributes": {
              "id": "venue-map"
            }
          },
          {
            "type": "LazyLoad",
            "props": {
              "src": "/venue.js",
              "children": [
                {
                  "type": "script",
                  "attributes": {
                    "x-attr": "",
                    "async": "",
                    "@src": "state.src",
                    "type": "module"
                  }
                }
              ]
            }
          },
          {
            "type": "AllSpeakers",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          },
          {
            "type": "div",
            "children": [
              {
                "type": "h2",
                "class": "text-4xl font-primary text-primary",
                "children": "Buy tickets"
              },
              {
                "type": "LazyLoad",
                "props": {
                  "src": "https://js.tito.io/v2",
                  "children": [
                    {
                      "type": "script",
                      "attributes": {
                        "x-attr": "",
                        "async": "",
                        "@src": "state.src"
                      }
                    },
                    {
                      "type": "tito-widget",
                      "attributes": {
                        "event": "future-frontend/2023"
                      }
                    }
                  ]
                }
              }
            ]
          },
          {
            "type": "Subscribe"
          },
          {
            "type": "AllSponsors",
            "bindToProps": {
              "conference": {
                "utility": "get",
                "parameters": [
                  "context",
                  "conference"
                ]
              }
            }
          }
        ]
      }
    ]
  }
}
