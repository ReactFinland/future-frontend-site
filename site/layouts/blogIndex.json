{
  "type": "BaseLayout",
  "props": {
    "content": {
      "type": "div",
      "class": "md:mx-auto px-4 md:px-0 w-full lg:max-w-3xl prose lg:prose-xl",
      "children": [
        {
          "type": "ul",
          "foreach": [{
            "utility": "get",
            "parameters": ["context", "blogPosts"]
          }, {
            "type": "li",
            "class": "flex flex-col gap-2 border-primary border-2 rounded p-2 hover:bg-gray-100",
            "children": [
              {
                "type": "Link",
                "bindToProps": {
                  "children": {
                    "utility": "get",
                    "parameters": ["props", "data.title"]
                  },
                  "href": {
                    "utility": "concat",
                    "parameters": [
                      "blog/",
                      {
                        "utility": "get",
                        "parameters": ["props", "data.slug"]
                      }
                    ]
                  }
                }
              },
              {
                "type": "time",
                "class": "font-extralight",
                "children": {
                  "utility": "getFullDate",
                  "parameters": [
                    {
                      "utility": "get",
                      "parameters": ["props", "data.date"]
                    }
                  ]
                },
                "attributes": {
                  "datetime": {
                    "utility": "get",
                    "parameters": ["props", "data.date"]
                  }
                }
              }
            ]
          }]
        }
      ]
    }
  }
}
