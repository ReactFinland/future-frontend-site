{
  "type": "BaseLayout",
  "props": {
    "content": {
      "type": "div",
      "class": "md:mx-auto px-4 md:px-0 w-full lg:max-w-3xl",
      "children": [
        {
          "type": "ul",
          "foreach": [{
            "utility": "get",
            "parameters": ["context", "blogPosts"]
          }, {
            "type": "li",
            "children": [
              {
                "type": "a",
                "class": "flex flex-col gap-2 border-primary border-2 rounded p-2 hover:bg-gray-100",
                "children": [
                  {
                    "type": "div",
                    "children": {
                      "utility": "get",
                      "parameters": ["props", "data.title"]
                    }
                  },
                  {
                    "type": "time",
                    "class": "font-extralight",
                    "children": {
                      "utility": "getFullDate",
                      "parameters": [
                        {
                          "utility": "get",
                          "parameters": ["props", "data.date"]
                        }
                      ]
                    },
                    "attributes": {
                      "datetime": {
                        "utility": "getDatetime",
                        "parameters": [
                          {
                            "utility": "get",
                            "parameters": ["props", "data.date"]
                          }
                        ]
                      }
                    }
                  }
                ],
                "attributes": {
                  "href": {
                    "utility": "concat",
                    "parameters": [
                      "blog/",
                      {
                        "utility": "get",
                        "parameters": ["props", "data.slug"]
                      }
                    ]
                  }
                }
              }
            ]
          }]
        }
      ]
    }
  }
}
